{
  "name": "obox_frontend",
  "version": "1.0.0",
  "description": "Obox frontend",
  "main": "index.js",
  "scripts": {
    "app": "cd app && npm run dev",
    "admin": "cd admin && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && cd admin && npm i && npm run build && cd ../app && npm i && npm run build && cd ../landing && npm run build && cd .. && npm run build-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir build/storybook",
    "install:app": "cd app && npm install",
    "install:admin": "cd admin && npm install",
    "install:shared": "cd shared && npm install",
    "install": "npm run install:app && npm run install:admin && npm run install:shared"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@rollup/plugin-alias": "5.0.0",
    "bootstrap": "5.3.0",
    "i18next": "23.2.11",
    "react-i18next": "13.0.2",
    "sass": "1.63.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.27",
    "@storybook/addon-interactions": "7.0.27",
    "@storybook/addon-links": "7.0.27",
    "@storybook/blocks": "7.0.27",
    "@storybook/react": "7.0.27",
    "@storybook/react-vite": "7.0.27",
    "@storybook/testing-library": "0.0.14-next.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.27"
  }
}
